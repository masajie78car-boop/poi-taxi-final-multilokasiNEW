{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/webhook$", "dest": "/index.js" },
    { "src": "/login.html", "dest": "/login.html" },
    { "src": "/panel_admin.html", "dest": "/panel_admin.html" },
    { "src": "/panel_admin_jatinegara.html", "dest": "/panel_admin_jatinegara.html" },
    { "src": "/(.*)", "dest": "/home.html" }
  ]
}
