{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "home.html", "use": "@vercel/static" },
    { "src": "login.html", "use": "@vercel/static" },
    { "src": "panel_admin.html", "use": "@vercel/static" },
    { "src": "panel_admin_jatinegara.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/webhook$", "dest": "index.js" },
    { "src": "^/api/webhook$", "dest": "index.js" },
    { "src": "/(.*)", "dest": "/home.html" }
  ]
}
